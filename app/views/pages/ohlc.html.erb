<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-6 home-module">
    <%= form_tag ohlc_import_path, multipart: true do %>
      <div>
        <h2>Import Dates/Symbols</h2>
        <div class='input-field'>
          <div class="file-field input-field">
            <div>
              <span><b><i>Input File</i></b>
                <%= file_field_tag :file %>
              </span>
            </div>
          </div>
        </div>
        <div class='input-field'>
          <div>
            <span><b>Output email address: </b></span><%= text_field_tag :email %>
          </div>
        </div>
        <div class='input-field'>
          <div>
            <span><b>Start index: </b></span><%= text_field_tag :start %>
          </div>
        </div>
      </div>
      <div style="padding-top:10px">
        <button type="submit" name="commit" value="Import" id='pi-submit-page' class='submit-btn'>
          Import
        </button>
      </div>
    <% end %>
  </div>
  <div class="col-md-3"></div>
</div>